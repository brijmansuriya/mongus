{
  "name": "prisma",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon --inspect index.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "seed": "node faker/SeedMaster.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security-check": "npm audit",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "prepare": "husky install",
    "build": "npm run lint && npm run test",
    "docker:build": "docker build -t mongus-api .",
    "docker:run": "docker run -p 3000:3000 mongus-api"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@faker-js/faker": "^9.0.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.2",
    "nodemon": "^3.1.4",
    "uuid": "^11.1.0",
    "uuid4": "^2.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-mongodb": "^6.0.0"
  },
  "devDependencies": {
    "compression": "^1.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1"
  }
}
